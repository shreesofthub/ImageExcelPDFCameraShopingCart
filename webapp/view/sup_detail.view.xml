<mvc:View controllerName="ns.businesspartners.controller.sup_detail"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m">
    <Page title="{i18n>detailTitle}" showNavButton="true" navButtonPress="onBack">
        <List>
            <items>
                <ObjectListItem icon="{photoUrl}" intro="{description}" number="{price}" numberState="Success" numberUnit="{unit}" title="{name}"></ObjectListItem>
            </items>
        </List>
    </Page>
</mvc:View>