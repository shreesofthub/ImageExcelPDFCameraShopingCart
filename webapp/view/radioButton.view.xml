<mvc:View controllerName="ns.businesspartners.controller.radioButton"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc">
    <Page id="page" title="{i18n>title}" showNavButton="true" navButtonPress="onBack">
        <headerContent  >
            <Button icon="sap-icon://customer" tooltip="USer Login" press="onLogin"></Button>
            <Button icon="sap-icon://cart-5" tooltip="Open Cart" press="onOpenCart"></Button>
            <ImageContent src="data:image/jpeg;base64," press="onCall"></ImageContent>
        </headerContent>
        <subHeader>
            <Toolbar >
                <SearchField showRefreshButton="true" width="auto" search=".onSearch" tooltip="Search based on Fruit Name/Description"></SearchField>
                <ToolbarSpacer/>
                <Button id="idsortButton" press=".onDialogOpen" icon="sap-icon://sort"/>
                <Button id="idfilterButton" press=".onDialogOpen" icon="sap-icon://filter"/>
                <Button id="idgroupButton" press=".onDialogOpen" icon="sap-icon://group-2"/>
            </Toolbar>
        </subHeader>
        <content>
            <List noDataText="Drop list items here" id="list0" items="{/fruits}" itemPress="onItemSelection">
                <items>
                    <ObjectListItem type="Navigation" title="{id}-{name}" icon="{photoUrl}" intro="{description}" number="{price}" numberUnit="{unit}">
                        <firstStatus>
                            <ObjectStatus id="idCart" icon="sap-icon://cart-4" active="true" press="onAddition" tooltip="Add to Cart">
                            </ObjectStatus>
                        </firstStatus>
                    </ObjectListItem>
                </items>
            </List>
        </content>
        <footer>
            <OverflowToolbar>
                <ToolbarSpacer/>
                <Button icon="sap-icon://sort" tooltip="This Sorting is based on ID-in My Way" press="onMySort"/>
            </OverflowToolbar>
        </footer>
    </Page>
</mvc:View>